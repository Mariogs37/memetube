<% @rows.each do |row| %>
  <section id="modify">
    <form action='/videos/<%= row['id'] %>/delete' method='post'>
      <button class="btn btn-danger">Delete</button>
      <a class="btn btn-warning" href='/videos/<%= row['id'] %>/edit/'>Edit</a>
    </form>

    <p><strong>Title:</strong> <%= row['title'] %></p>
    <p><strong>Description:</strong> <%= row['description'] %></p>
    <p><strong>Genre:</strong> <%= row['genre'] %></p>
    <iframe width="420" height="315" src='<%= row['URL'] %>' frameborder="0" allowfullscreen></iframe>

  </section>
<% end %>
