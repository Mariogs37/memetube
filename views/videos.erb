<% @rows.each do |row| %>
  <section id="modify">
    <form action='/videos/<%= row['id'] %>/delete' method='post'>
      <button class="btn btn-danger">Delete</button>
      <a class="btn btn-warning" href='/videos/<%= row['id'] %>/edit/'>Edit</a>
    </form>

    <strong>Title:</strong> <%= row['title'] %>
    <br>
    <strong>Description:</strong> <%= row['description'] %>
    <br>
    <strong>Genre:</strong> <%= row['genre'] %>
    <br>
    <iframe width="420" height="315" src='<%= row['url'] %>' frameborder="0" allowfullscreen></iframe>

  </section>
<% end %>